		<recipe name="Quick Lemonade"
			qty="1"
			uom_p="litres"
			uom_s="litre"
			time="5 mins"
			tested="yes"
			rating="7"
			last-modified="21/07/2003">
			
			<ingredients>
				<group id="A">
					<ingredient
						name="Lemon Juice"
						qty="250"
						uom="ml"
						cost="0.62" />
					<ingredient
						name="Water"
						qty="700"
						uom="ml"
						cost="0" />
					<ingredient
						name="Fructose"
						qty="4"
						uom="tbsp"
						cost="0.20" />
