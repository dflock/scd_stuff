					<ingredient
						name="each of Basil &amp; Oregano"
						qty="1"
						uom="tsp"
						cost="0.10" />
				</group>
				<group id="D">
					<ingredient
						name="grated Cheese"
						qty="1-2"
						uom="cup"
						cost="0.55" />
					<ingredient
						name="herbs, to taste."
