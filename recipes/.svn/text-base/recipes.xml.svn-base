			uom_s="pizza"
			uom_p="pizza"
			time="45 mins"
			tested="yes"
			rating="9"
			last-modified="27/05/2004">
			
			<ingredients>
				<group id="A">
					<ingredient
						name="Almond Flour"
						qty="1"
						uom="cup"
						cost="1.00" />
					<ingredient
						name="Egg"
						qty="1"
						uom=""
						cost="0.12" />
					<ingredient
						name="Salt"
						qty="0.3"
